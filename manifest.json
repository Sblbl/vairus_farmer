{
	"name": "FARM VAIRUSES",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Opens your Vairus node pages every 30 minutes",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": [
			"src/bg/background.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_title": "VAIRUS FARMER",
		"default_popup": "src/browser_action/browser_action.html"
	},
	"permissions": [
		"storage", 
		"tabs"
	]
}